{
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Gruvbox Dark (Hard)",
    "explorer.openEditors.visible": 0,
    "editor.minimap.enabled": false,
    "editor.tabCompletion": "on",
    "vim.useSystemClipboard": true,
    "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "jest.autoRun": {},
    "vim.easymotion": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<space>"
            ],
            "commands": [
                "vspacecode.space"
            ]
        },
        {
            "before": [
                ","
            ],
            "commands": [
                "vspacecode.space",
                {
                    "command": "whichkey.triggerKey",
                    "args": "m"
                }
            ]
        }
    ],
    "vspacecode.bindingOverrides": [
        {
            "keys": "s.n",
            "name": "Next Search Result",
            "type": "command",
            "command": "search.action.focusNextSearchResult"
        },
        {
            "keys": "s.p",
            "name": "Previous Search Result",
            "type": "command",
            "command": "search.action.focusPreviousSearchResult"
        },
        {
            "keys": "u",
            "name": "Unittests",
            "type": "bindings",
            "bindings": [
                {
                    "key": "u",
                    "name": "Run current test",
                    "icon": "run",
                    "type": "command",
                    "command": "testing.runAtCursor"
                },
                {
                    "key": "t",
                    "name": "Open test explorer",
                    "icon": "list-selection",
                    "type": "command",
                    "command": "workbench.view.testing.toggleVisibility"
                },
                {
                    "key": "r",
                    "name": "Re-run last test",
                    "icon": "debug-restart",
                    "type": "command",
                    "command": "testing.reRunLastRun"
                },
                {
                    "key": "f",
                    "name": "Debug fail tests",
                    "icon": "error",
                    "type": "command",
                    "command": "testing.debugFailTests"
                },
                {
                    "key": "l",
                    "name": "Show test log",
                    "icon": "list-flat",
                    "type": "command",
                    "command": "testing.debugAtCursor"
                },
                {
                    "key": "b",
                    "name": "Run current test file",
                    "icon": "file",
                    "type": "command",
                    "command": "testing.runCurrentFile"
                },
                {
                    "key": "c",
                    "name": "Cancel running tests",
                    "icon": "close",
                    "type": "command",
                    "command": "testing.cancelRun"
                }
            ]
        }
    ],
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderWhitespace": "none",
    "magit.forge-enabled": true,
    "workbench.activityBar.visible": false
}
